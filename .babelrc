{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {"node": "current"},
        "useBuiltIns": "usage",
        "corejs": {"version": 3, "proposals": true}
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-proposal-object-rest-spread",
    [
      "@babel/plugin-transform-runtime",
      {"regenerator": true}
    ]
  ]
}